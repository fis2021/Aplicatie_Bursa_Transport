<AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: seashell;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.app.transport.controllers.GoodListController">
   <children>
      <ListView fx:id="listView" layoutX="129.0" prefHeight="400.0" prefWidth="371.0" />
