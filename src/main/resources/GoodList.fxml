                  <FontAwesomeIconView glyphName="INFO" />
               </graphic>
            </Button>
            <Button mnemonicParsing="false" prefHeight="91.0" prefWidth="118.0" style="-fx-background-color: gray;" text="Propose &#10;   offer" textFill="#f2eeee">
               <graphic>
                  <FontAwesomeIconView glyphName="USD" />
               </graphic>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
