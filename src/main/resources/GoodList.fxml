      <VBox prefHeight="400.0" prefWidth="129.0" style="-fx-background-color: gray;">
         <children>
            <Button fx:id="ReturnHome" mnemonicParsing="false" onMouseClicked="#handleReturn" prefHeight="79.0" prefWidth="132.0" style="-fx-background-color: gray;" text="Return To &#10;Home Page" textFill="#f2f0f0">
               <graphic>
                  <FontAwesomeIconView glyphName="BACKWARD" />
               </graphic>
            </Button>
            <Button fx:id="refresh" mnemonicParsing="false" onMouseClicked="#handleRefresh" prefHeight="71.0" prefWidth="114.0" style="-fx-background-color: gray;" text="Refresh" textFill="#fffafa">
               <graphic>
                  <FontAwesomeIconView glyphName="REFRESH" />
               </graphic>
